{
    "manifest_version": 2,
    "name": "My First Extension",
    "version": "1.0",
    "description": "This is my first Chrome extension.",
    "permissions": [
        "storage",
        "cookies", 
        "https://*.roblox.com/" 
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html",
    },
  
    "content_security_policy": "script-src 'self'; object-src 'self'"
}
